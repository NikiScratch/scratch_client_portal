<div class="white-pink">
<h1>History of Events for <%=current_user.name%> </h1>

<table>
    
    </tr>
        <td><b><%= "Name" %></b> </td>
        <td><b><%= "Date" %></b></td>
        <td><b><%= "Time" %></b></td>
        <td><b><%= "DJ Name" %></b></td>
    <% @events.each do |event| %>
    <tr>
       
        <td><%= event.name%> </td>
        <td><%= event.date.strftime("%b %d, %Y") %></td>
        <td><%= event.time.strftime("%I:%Mp") %></td>
        <td><%= event.talents.name%> </td>
        
       
    </tr>

    <% end %>
</table>

</div>